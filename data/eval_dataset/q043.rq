# category: "Works and Expressions"
# split: "easy"
# origin: user_question
# question: "Where is Mozart born?"
# query:
SELECT DISTINCT ?placeLabel
WHERE {
?artist a ecrm:E21_Person .
?artist foaf:name ?name .
VALUES ?artist { <http://data.doremus.org/artist/4802a043-23bb-3b8d-a443-4a3bd22ccc63> }
?artist schema:birthPlace ?birthPlace .
?birthPlace rdfs:label ?placeLabel . 
}

