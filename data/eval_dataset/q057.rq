# question: "Give me performances held in Paris."
# used_tools: ["find_candidate_entities", "build_query", "apply_filter", "add_select", "execute_query"]
# query:
SELECT DISTINCT ?performance ?title
WHERE {
  ?performance a efrbroo:F31_Performance ;
          rdfs:label ?title ;
          ecrm:P7_took_place_at ?place .
  ?place geonames:name ?placeName .
  FILTER ( REGEX(?placeName, 'Paris', 'i') )
}

