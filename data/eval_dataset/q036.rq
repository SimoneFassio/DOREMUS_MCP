# category: "Recordings"
# split: "medium"
# origin: competency_question
# question: "Give me all the recordings of Don Giovanni by Mozart"
# query:
SELECT DISTINCT ?rec
WHERE {
  ?rec mus:U51_is_partial_or_full_recording_of ?perf.

  ?perf rdfs:label ?label ;
         mus:U54_is_performed_expression_of	?work .

  ?work rdfs:label ?title.

  ?expCreation efrbroo:R17_created ?work ;
          ecrm:P9_consists_of / ecrm:P14_carried_out_by <http://data.doremus.org/artist/4802a043-23bb-3b8d-a443-4a3bd22ccc63> .

  FILTER (contains(str(?title), "Don Giovanni"))
}

# workflow
build_query(question="Give me all the recordings of Don Giovanni by Mozart", template="track")
apply_filter(query_id=query_id, base_variable="track", template="track", filters={"work_title": "Don Giovanni", "composer_name": "Wolfgang Amadeus Mozart"})
execute_query(query_id=query_id)