# category: "Artists"
# split: "easy"
# origin: user_question
# question: "Find artists with 'Simone' in their name."
# query:
SELECT DISTINCT ?artist ?name
WHERE {
  ?artist a ecrm:E21_Person ;
          foaf:name ?name .
  FILTER REGEX(?name, "simone", "i")
}

