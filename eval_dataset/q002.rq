# category: "Works and Expressions"
# split: "medium"
# origin: competency_question
# question: "Which works have been composed in 1836 ?"
# query:
SELECT DISTINCT ?expression, sql:BEST_LANGMATCH(?title, 'en, en-gb;q=0.8, fr=0.6; *;q=0.1', 'en') as ?title, ?start, ?end
WHERE {
  ?expression a efrbroo:F22_Self-Contained_Expression ;
          rdfs:label ?title .

  ?expCreation efrbroo:R17_created ?expression ;
          ecrm:P4_has_time-span / time:hasEnd / time:inXSDDate ?end ;
          ecrm:P4_has_time-span / time:hasBeginning / time:inXSDDate ?start .

  FILTER ( ?start <= "1836"^^xsd:gYear AND ?end >= "1836"^^xsd:gYear )
}

# workflow
build_query(question="Which works have been composed in 1836 ?", template="expression")
filter_by_quantity(subject="expCreation", property="ecrm:P4_has_time-span", type="range", value="1836", valueEnd="1836", query_id=query_id)
execute_query(query_id=query_id)